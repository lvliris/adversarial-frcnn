    stride: 2
  }
}
layer {
  name: "conv5_1"
  type: "Convolution"
  bottom: "pool4"
  top: "conv5_1"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu5_1"
  type: "ReLU"
  bottom: "conv5_1"
  top: "conv5_1"
}
layer {
  name: "conv5_2"
  type: "Convolution"
  bottom: "conv5_1"
  top: "conv5_2"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu5_2"
  type: "ReLU"
  bottom: "conv5_2"
  top: "conv5_2"
}
layer {
  name: "conv5_3"
  type: "Convolution"
  bottom: "conv5_2"
  top: "conv5_3"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
  }
}
layer {
  name: "relu5_3"
  type: "ReLU"
  bottom: "conv5_3"
  top: "conv5_3"
}

layer {
  name: "roi_pool5"
  type: "ROIPooling"
  bottom: "conv5_3"
  bottom: "rois"
  top: "pool5"
  propagate_down: true
  propagate_down: false
  roi_pooling_param {
    pooled_w: 7
    pooled_h: 7
    spatial_scale: 0.0625 # 1/16
  }
}

layer {
  name: "astn_pretrain_selectlabels"
  type: "Python"
  bottom: "pool5"
  bottom: "labels"
  bottom: "rois"
  top: "conv_feat_pos"
  top: "labels_pos"
  top: "rois_pos"

  propagate_down: false
  propagate_down: false
  propagate_down: false

  python_param {
    module: "roi_data_layer.layer"
    layer: "ASDNPretrainLabelLayer"
  }
}

# astn branch, the first two layers initialized from ImageNet fc6, fc7

layer {
  name: "fc6_astn"
  type: "InnerProduct"
  bottom: "conv_feat_pos"
  top: "fc6_astn"
  param {
    name: "fc6_astn_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "fc6_astn_b"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "relu6_astn"
  type: "ReLU"
  bottom: "fc6_astn"
  top: "fc6_astn"
}
layer {
  name: "drop6_astn"
  type: "Dropout"
  bottom: "fc6_astn"
  top: "fc6_astn"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "fc7_astn"
  type: "InnerProduct"
  bottom: "fc6_astn"
  top: "fc7_astn"
  param {
    name: "fc7_astn_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "fc7_astn_b"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "relu7_astn"
  type: "ReLU"
  bottom: "fc7_astn"
  top: "fc7_astn"
}
layer {
  name: "drop7_astn"
  type: "Dropout"
  bottom: "fc7_astn"
  top: "fc7_astn"
  dropout_param {
    dropout_ratio: 0.5
  }
}

layer {
  name: "fc8_astn"
  type: "InnerProduct"
  bottom: "fc7_astn"
  top: "trans_param"
  param {
    name: "trans_param_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "trans_param_b"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "astn_data"
  type: "Python"
  bottom: "trans_param"
  bottom: "conv_feat_pos"
  top: "trans_feat"


  python_param {
    module: "roi_data_layer.layer"
    layer: "ASTNDataLayer"
    param_str: "{\'channels\': 512, \'block_num\': 4}"
  }
}



#### classification branch after masking
layer {
  name: "fc6"
  type: "InnerProduct"
  bottom: "trans_feat"
  top: "fc6"
  param {
    name: "fc6_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "fc6_b"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "relu6"
  type: "ReLU"
  bottom: "fc6"
  top: "fc6"
}
layer {
  name: "drop6"
  type: "Dropout"
  bottom: "fc6"
  top: "fc6"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "fc7"
  type: "InnerProduct"
  bottom: "fc6"
  top: "fc7"
  param {
    name: "fc7_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "fc7_b"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 4096
  }
}
layer {
  name: "relu7"
  type: "ReLU"
  bottom: "fc7"
  top: "fc7"
}
layer {
  name: "drop7"
  type: "Dropout"
  bottom: "fc7"
  top: "fc7"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  name: "cls_score"
  type: "InnerProduct"
  bottom: "fc7"
  top: "cls_score"
  param {
    name: "cls_score_w"
    lr_mult: 1
    decay_mult: 1
  }
  param {
    name: "cls_score_b"
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 21
    weight_filler {
      type: "gaussian"
      std: 0.01
    }
    bias_filler {
      type: "constant"
      value: 0
    }
  }
}

layer {
  name: "cls_prob"
  type: "Softmax"
  bottom: "cls_score"
  top: "cls_prob"
  propagate_down: false
}


layer {
  name: "loss_cls"
  type: "SoftmaxWithLoss"
  bottom: "cls_score"
  bottom: "labels_pos"
  top: "loss_cls"
  propagate_down: true
  propagate_down: false
  loss_weight: 1
}


# loss for astn
layer {
  name: "loss_astn_pretrain"
  type: "Python"
  bottom: "cls_prob"
  bottom: "labels_pos"
  top: "loss_astn"
  propagate_down: true
  propagate_down: false
  python_param {
    module: "roi_data_layer.layer"
    layer: "ASTNLossLayer"
    param_str: "{\'num_classes\':21, \'score_thres\':0.1 }"
  }
  loss_weight: 0.0025
}

